        },
        "Environment": {
          "ComputeType": "BUILD_GENERAL1_SMALL",
          "EnvironmentVariables": [
            {
              "Name": "BUCKET",
              "Type": "PLAINTEXT",
              "Value": {
                "Fn::ImportValue": "lambda-bucket"
              }
            }
          ],
          "Image": "aws/codebuild/standard:3.0",
